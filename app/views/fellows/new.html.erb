<div class="container profile fellow">
  <%= render partial: 'shared/notice' %>
  <%= render 'profiles/form', profile: @profile do |f| %>
    <div class="form-group">
      <div class="btn-group col-sm-9 col-sm-offset-3" role="group">
        <input type="submit" class="btn btn-default" value="Save" />
        <%= link_to 'Cancel', fellows_path, class: 'btn btn-default' %>
      </div>
    </div>
  <% end %>
</div>

